{
  "name": "learn-nestjs-prisma",
  "version": "0.0.1",
  "description": "A monorepo with NestJS backend, React web app, and Expo mobile app",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm -r build",
    "build:service": "pnpm --filter=@learn-nestjs-prisma/service build",
    "build:web": "pnpm --filter=@learn-nestjs-prisma/web build",
    "service:dev": "pnpm --filter=@learn-nestjs-prisma/service start:dev",
    "web:dev": "pnpm --filter=@learn-nestjs-prisma/web dev",
    "service:start": "pnpm --filter=@learn-nestjs-prisma/service start",
    "service:prod": "pnpm --filter=@learn-nestjs-prisma/service start:prod",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "db:generate": "pnpm --filter=@learn-nestjs-prisma/service db:generate",
    "db:migrate:dev": "pnpm --filter=@learn-nestjs-prisma/service db:migrate:dev",
    "db:seed": "pnpm --filter=@learn-nestjs-prisma/service db:seed",
    "clean": "pnpm -r clean",
    "install:all": "pnpm install"
  },
  "devDependencies": {
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
